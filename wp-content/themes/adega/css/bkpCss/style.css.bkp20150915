/* page parceiros */
.parceiros {
	padding: 40px 40px 0;
	text-align: center;
	color: #646464;
	font-size: 16px;
	line-height: 26px;
	font-weight: 400;
}
.box-parceiros {
	margin: 15px 0;
	background-color: #ffffff;
	width: 100%;
	padding: 70px 0;
	position: relative;
	-webkit-box-shadow: 4px 4px 5px 0px rgba(32,29,30,0.35);
	-moz-box-shadow: 4px 4px 5px 0px rgba(32,29,30,0.35);
	box-shadow: 4px 4px 5px 0px rgba(32,29,30,0.35);
}
.box-parceiros:hover .box-parceiros-hover {
	opacity: 1;
    -webkit-transition: all 500ms ease 0s;
    -moz-transition: all 500ms ease 0s;
    -o-transition: all 500ms ease 0s;
    transition: all 500ms ease 0s;
}
.box-parceiros-hover {
	position: absolute;
	top:0;
	left: 0;
	width: 100%;
	height: 100%;
	display: block;
	padding:80px 0 0;
	background-color: rgba(204,148,27,0.85);
	opacity: 0;
	-webkit-transition: all 500ms ease 0s;
    -moz-transition: all 500ms ease 0s;
    -o-transition: all 500ms ease 0s;
    transition: all 500ms ease 0s;
}
.box-parceiros-hover h3 {
	font-size: 24px;
	line-height: 26px;
	color: #ffffff;
	font-weight: 700;
}
.box-parceiros-hover h4 {
	font-size: 20px;
	line-height: 26px;
	color: #ffffff;
}
.box-parceiros-hover a {
