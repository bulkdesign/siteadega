/* GERAL */

@import url('extra.css');

body {
    background-color: #ffffff;
    background-image: url('../images/bg.jpg');
    font-family: 'Lato', sans-serif;
    color: #333;
    font-size: 14px;
    line-height: 20px;
}
.h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6 {
	font-family: 'Lato', sans-serif;
    color: #333;
}
ul {
	list-style: none;
	padding-left: 0;
}
a:hover {
	text-decoration: none;
	color: #808285;
	-webkit-transition: .3s, ease !important;
	   -moz-transition: .3s, ease !important;
	    -ms-transition: .3s, ease !important;
	     -o-transition: .3s, ease !important;
	        transition: .3s, ease !important;
}
a:focus {
	outline: none;
	text-decoration: none;
}
.btn {
	border:0 none;
}
.form-control {
	box-shadow: none;
	border-color: #b3b5b7;
}

/* chat */
.box-chat {
	position: fixed;
	top: 50%;
	right: 0;
	z-index: 2;
}

/* header */
.navbar {
	height: 130px;
	background-color: #f1f1f1;
	margin-bottom: 0;
	box-shadow: 5px 0 7px 0 #d8d1b8;
	-webkit-box-shadow: 5px 0 7px 0 #d8d1b8;
	-moz-box-shadow: 5px 0 7px 0 #d8d1b8;
}
.navbar-brand {
	background-image: url('../images/logo.jpg');
	background-repeat: no-repeat;
	background-size: 100%;
	width: 170px;
	height: 80px;
	display: block;
	text-indent: -5000px;
	margin-top: 40px;
	margin-left: 10px;
}
.navbar-default .navbar-toggle {
	border:0 none;
	margin-top: 70px;
	margin-right: 60px;
}
.navbar-default .navbar-toggle .icon-bar {
	background-color: #cc941b;
}

/* menu */
.navbar .container {position: relative;}
.navbar .navbar-collapse {
	background-color: #f1f1f1;
	width: 100%;
	position: absolute;
}
.navbar-default .navbar-nav > li > a {
	color: #231f20;
	font-family: 'Lato', sans-serif;
	font-size: 14px;
	font-weight: 700;
	padding: 15px 0 5px;
	margin: 0 15px;
	text-transform: uppercase;
}
.navbar-default .navbar-nav > .current_page_item > a, 
.navbar-default .navbar-nav > .current_page_item > a:focus, 
.navbar-default .navbar-nav > .current_page_item > a:hover {
	background-color: transparent;
	color: #cc941b;
	text-decoration: none;
	border-bottom: 2px solid #cc941b;
}
.navbar-default .navbar-nav > .active > a, 
.navbar-default .navbar-nav > .active > a:focus, 
.navbar-default .navbar-nav > .active > a:hover {
	background-color: transparent;
	color: #cc941b;
	text-decoration: none;
	border-bottom: 2px solid #cc941b;
}
.navbar-default .navbar-nav > li > a:focus, 
.navbar-default .navbar-nav > li > a:hover {
	background-color: transparent;
	color: #cc941b;
	text-decoration: none;
	border-bottom: 2px solid #cc941b;
}
.navbar-default .navbar-nav > .open > a, 
.navbar-default .navbar-nav > .open > a:focus, 
.navbar-default .navbar-nav > .open > a:hover {
	background-color: transparent;
	color: #cc941b;
}
.navbar-default .navbar-nav > li:hover > a{
	background-color: transparent;
	color: #cc941b;
	text-decoration: none;
	border-bottom: 2px solid #cc941b;
}
.navbar-default .navbar-nav > li:hover .dropdown-menu{
	opacity: 1;
	visibility: visible;
}
.navbar-default .navbar-nav > li > .sub-menu{
	visibility: hidden;
	opacity: 0;
}
.dropdown-menu {
	display: block;
	border: 0 none;
	background-color: #f1f1f1;
	box-shadow: none;
	-webkit-box-shadow: none;
	padding: 0;
	
	opacity: 0;
	visibility: hidden;
}
.dropdown-menu > li {
	border-bottom: 1px solid #ffffff;
}
.dropdown-menu > li > a {
	color: #979697;
	text-transform: uppercase;
	padding: 7px 20px;
}
.dropdown-menu > li > a:focus, 
.dropdown-menu > li > a:hover {
	color: #ffffff;
	background-color: #cc941b;
}

/* busca */
.busca {
	float: right;
	position: absolute;
	right: 15px;
	top: 70px;
}
.busca .searchandfilter ul li{
	padding: 0;
}
.input-busca {
	border: 1px solid #afb0b3;
	background-color: #ffffff;
	background-image: url('../images/icon-busca.png');
	background-position: 10px center;
	background-repeat: no-repeat;
	color: #c5c5c5;
	width: 30px;
	height: 29px;
	padding-left: 25px;
	font-family: 'Lato', sans-serif;
	font-size: 13px;
	line-height: 29px;
	margin-top: 0;
}
li.sf-field-search{
	padding: 0;
}

/* banner */
.carousel-banner {
	margin-bottom: 10px;
}
.carousel-banner .banner {
	background-color: #211e02;
}

/* Variacoes de cores do banner */
.carousel-banner .banner.yellow {
	background-color: #f1e4d1;
}


.carousel-banner img {
	position: relative;
	left: 50%;
	height: 220px;
	margin-left: -340px;
}
.carousel-banner .content-banner {
	position: absolute;
	top: 50%;
	width: 100%;
	text-align: center;
	padding: 10px 0;
	background-color: rgba(53, 64, 6, 0.65);
	color: rgba(53, 64, 6, 0.65);
}
.carousel-banner .content-banner.dois {
	background-color: rgba(204, 148, 27, 0.70);
	color: rgba(204, 148, 27, 0.70);
}
.carousel-banner .content-banner h2 {
	text-transform: uppercase;
	color: #ffffff;
	font-size: 18px;
	line-height: 20px;
	font-weight: 100;
	letter-spacing: 3px;
	margin: 0;
}
.carousel-banner .content-banner p {
	color: #ffffff;
	margin: 0;
	font-size: 12px;
	line-height: 18px;
	font-weight: 400;
}

/* fotos */
.box-fotos .galeria-fotos img {
	margin: 0 auto;
}

/* titulos */
.box-sombra {
	width: 100%;
	display: inline-block;
	-webkit-box-shadow: inset 0px -6px 7px 0px rgba(191,180,161,0.84);
	-moz-box-shadow: inset 0px -6px 7px 0px rgba(191,180,161,0.84);
	box-shadow: inset 0px -6px 7px 0px rgba(191,180,161,0.84);
}
.box-title {
	margin: 55px auto;
    width:100%;
    height:1px;
    background-color:#414042;
    line-height:1px;
    text-align:center;
}
.title {
	font-size: 30px;
	font-weight: 700;
	text-transform: uppercase;
	color: #414042;
	text-align: center;
	letter-spacing: 2px;
	padding:7px 25px;
	background-image: url('../images/bg.jpg');
    background-color:#ffffff;
    display:inline-block;
    margin-top: -20px;
    min-width: 240px;
}
.box-produtos .title {
	background-color:#F3F3F3;
	background-image: none;
}


/* produtos */
.box-produtos {
	width: 100%;
	margin: 0 auto;
	padding: 20px 0;
	display: inline-block;
	vertical-align: top;
	background: -moz-linear-gradient(top,  rgba(229,229,229,0.5) 0%, rgba(229,229,229,0.5) 2%, rgba(255,255,255,0.1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(229,229,229,0.5)), color-stop(2%,rgba(229,229,229,0.5)), color-stop(100%,rgba(255,255,255,0.1)));
	background: -webkit-linear-gradient(top,  rgba(229,229,229,0.5) 0%,rgba(229,229,229,0.5) 2%,rgba(255,255,255,0.1) 100%);
	background: -o-linear-gradient(top,  rgba(229,229,229,0.5) 0%,rgba(229,229,229,0.5) 2%,rgba(255,255,255,0.1) 100%);
	background: -ms-linear-gradient(top,  rgba(229,229,229,0.5) 0%,rgba(229,229,229,0.5) 2%,rgba(255,255,255,0.1) 100%);
	background: linear-gradient(to bottom,  rgba(229,229,229,0.5) 0%,rgba(229,229,229,0.5) 2%,rgba(255,255,255,0.1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#80e5e5e5', endColorstr='#80ffffff',GradientType=0 );
}
.box-produtos .galeria-produtos img {
	margin: 0 auto;
}

/* diferenciais */
.item-diferencial {
	text-align: center;
